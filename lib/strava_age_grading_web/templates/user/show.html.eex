<h1>Show User</h1>

<ul>

  <li>
    <strong>Email.string:</strong>
    <%= @user.email.string %>
  </li>

  <li>
    <strong>Access token:</strong>
    <%= @user.access_token %>
  </li>

  <li>
    <strong>Refresh token:</strong>
    <%= @user.refresh_token %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.user_path(@conn, :edit, @user) %></span>
<span><%= link "Back", to: Routes.user_path(@conn, :index) %></span>
